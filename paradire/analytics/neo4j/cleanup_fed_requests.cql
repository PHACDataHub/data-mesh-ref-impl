/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
//
// Removing Vaccination Records
//
/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
CALL apoc.periodic.iterate('
	MATCH (n:VaccinationRecord)
	RETURN n
','
    WITH n DETACH DELETE n
',
	{batchSize:10000, parallel:true}
) YIELD total RETURN total;
//
/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

